@model IEnumerable<AjaxDemo.Models.Product>

<table border="1">
    <tr>
        <th>Product Id</th>
        <th>Product Name</th>
        <th>Product Price</th>
    </tr>
    
    @foreach(var item in Model)
    {
       <tr>
            <td>@item.ProductId</td>
        <td>@item.Name</td>
        <td>@item.Price</td>
       </tr>
    }
</table>
